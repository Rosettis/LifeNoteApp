<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <Button
            android:id="@+id/btn_termGraph"
            android:layout_width="179dp"
            android:layout_height="wrap_content"
            android:text="Term Use"
            />

        <Button
            android:id="@+id/btn_illnessGraph"
            android:layout_width="179dp"
            android:layout_height="wrap_content"
            android:text="Illness History"/>


    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout_graphHolder"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context="capstone.uoit.ca.lifenoteapp.functions.Graphs.GraphHome">

        <!--<com.github.mikephil.charting.charts.HorizontalBarChart-->
        <!--android:id="@+id/chart"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="match_parent" />-->

    </LinearLayout>
</LinearLayout>


